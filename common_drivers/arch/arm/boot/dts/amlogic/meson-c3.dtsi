				interrupts = <GIC_SPI 184 IRQ_TYPE_LEVEL_HIGH>;
				clock-names = "core", "async";
				clocks = <&clkc CLKID_SYS_SPICC_1>,
					 <&clkc CLKID_SPICC_B>;
				#address-cells = <1>;
				#size-cells = <0>;
				//power-domains = <&pwrdm PDID_CX_SPICC_B>;
				status = "disabled";
