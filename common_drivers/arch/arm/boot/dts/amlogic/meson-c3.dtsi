		mboxes = <&mbox_pl 1>,
			 <&mbox_pl 3>;
		mbox-dests = <MAILBOX_DSP>,
			     <MAILBOX_AOCPU>;
	};

	rng{
		compatible="amlogic,meson-rng";
		status="okay";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0xfe440788 0x0c>;
		quality=/bits/ 16 <1000>;
		version = <2>;
	};
};

