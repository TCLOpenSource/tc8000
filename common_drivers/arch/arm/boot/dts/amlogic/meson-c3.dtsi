				reg = <0x50000 0x44>;
				interrupts = <GIC_SPI 183 IRQ_TYPE_LEVEL_HIGH>;
				clock-names = "core", "async";
				clocks = <&clkc CLKID_SYS_SPICC_0>,
					 <&clkc CLKID_SPICC_A>;
				#address-cells = <1>;
				#size-cells = <0>;
